            <div class="container">

                <div class="row">
                    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3" style="margin-left: 63px;">
                        <form role="form" method="post">
                            <h2>Please Sign Up <small>It's free and always will be.</small></h2>
                            <hr class="colorgraph">
                            <div class="row">
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="text" name="signup_name" id="signup_name" class="form-control input-lg" placeholder="Name" tabindex="1" required autofocus>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="text" name="signup_id" id="signup_id" class="form-control input-lg" placeholder="Identification" tabindex="2" required>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <input type="text" name="signup_department" id="signup_department" class="form-control input-lg" placeholder="Department" tabindex="3" required>
                            </div>
                            <div class="form-group">
                                <input type="text" name="signup_position" id="signup_position" class="form-control input-lg" placeholder="Position" tabindex="4" required>
                            </div>
                            <div class="form-group">
                                <input type="text" name="signup_smartphone_address" id="signup_smarthphone_address" class="form-control input-lg" placeholder="Smartphone Address" tabindex="5" required>
                            </div>
                            <div class="row">
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="password" name="signup_pw" id="signup_pw" class="form-control input-lg" placeholder="Password" tabindex="6" required>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="password" name="signup_pw_confirm" id="password_confirm" class="form-control input-lg" placeholder="Confirm Password" tabindex="7" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-4 col-sm-3 col-md-3">
          					<span class="button-checkbox">
          						<button type="button" class="btn" data-color="info" tabindex="8">I Agree</button>
                                <input type="checkbox" name="t_and_c" id="t_and_c" class="hidden" value="1">
          					</span>
                                </div>
                                <div class="col-xs-8 col-sm-9 col-md-9">
