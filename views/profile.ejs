    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->


        <!-- Main content -->
        <section class="content">

            <div class="container">

                <div class="row">

                    <div class="alert alert-info alert-dismissable" style="
    width: 1022px;
    margin-left: 0px;
    margin-right: 0px;
">
                        <a class="panel-close close" data-dismiss="alert">×</a>

                        정보를 입력해주세요
                    </div>
                    <!-- left column -->
                    <div class="col-md-4 col-sm-6 col-xs-12" style="top: 82px;">
                        <div class="text-center">
                            <img src="http://lorempixel.com/200/200/people/9/" class="avatar img-circle img-thumbnail">

                        </div>
                    </div>
                    <!-- edit form column -->
                    <div class="col-md-8 col-sm-6 col-xs-12 personal-info">

                        <!--14.63.219.156/profile?
                        name=asd&employee_number=asd&workplace=&smartphone_address=&position=&password=&password=# -->
                        <!-- 이건 프로필수정 페이지로 위에 값들은 데이터베이스의 identity에 있는 각각의 컬럼들의 값을 수정하는 모습이다
                        name,employee_number,workplace,smartphone_address,position,password 가 필요함-->

                        <form action="#" class="form-horizontal" role="form" style="margin-right:300px; margin-top:25px;" method ="post">
                            <div class="form-group">
                                <label class="col-lg-3 control-label">이름:</label>
                                <div class="col-lg-8">
                                    <input class="form-control" name="name"  type="text" maxlength="5">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-3 control-label">사원번호:</label>
                                <div class="col-lg-8">
                                    <input class="form-control" name="employee_number"  type="text" maxlength="10">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-3 control-label">회사명:</label>
                                <div class="col-lg-8">
                                    <input class="form-control" name="workplace" type="text" maxlength="10">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-lg-3 control-label">스마트폰주소:</label>
                                <div class="col-lg-8">
                                    <input class="form-control" name="smartphone_address" type="text" maxlength="16">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-3 control-label">직급:</label>
                                <div class="col-md-8">
                                    <input class="form-control" name="position"  type="text" maxlength="5">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-3 control-label">비밀번호:</label>
                                <div class="col-md-8">
                                    <input class="form-control" name="password"  type="password" maxlength="10" >
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-3 control-label">비밀번호확인:</label>
                                <div class="col-md-8">
                                    <input class="form-control" name="password" type="password">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-3 control-label"></label>
                                <div class="col-md-8">
                                    <input class="btn btn-primary" value="Save Changes" type="submit">
                                    <span></span>
                                    <input class="btn btn-default" value="Cancel" type="reset">
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>


