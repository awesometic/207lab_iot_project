                    }

                    rect {
                        fill-opacity: .6;
                        fill: #e377c2;
                    }

                    rect:first-child {
                        fill: #1f77b4;
                    }

                </style>
                <script src="//d3js.org/d3.v3.min.js"></script>
                <script>

                    var margin = {top: 0, right: 0, bottom: 0, left: 0},
                            width = 960 - margin.left - margin.right, // 900
                            height = 500 - margin.top - margin.bottom, //  450
                            barWidth = Math.floor(width / 7) - 1; // (47) - 1 => 46

                    var x = d3.scale.linear()
                            .range([barWidth / 2, width - barWidth / 2]); // 23, 867

                    var y = d3.scale.linear()
                            .range([height, 0]); // 450, 0

                    var yAxis = d3.svg.axis()
                            .scale(y)

                            .tickSize(-width) // -900
                            .tickFormat(function(d) { return Math.round(d) + "H"; }); // d / 1 * 10^6


                    // An SVG element with a bottom-right origin.
                    var svg = d3.select("body").append("svg")
                            .attr("width", width + margin.left + margin.right) //960?
                            .attr("height", height + margin.top + margin.bottom) //500?
                            .append("g")
                            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");

                    // A sliding container to hold the bars by birthw_count.
                    var birthw_counts = svg.append("g")
                            .attr("class", "birthw_counts");

                    var weeks = 34;

                    // A label for the current w_count.
                    var title = svg.append("text")
                            .attr("class", "title")
                            .attr("dy", ".71em")
                            .text(weeks + "weeks");

                    d3.csv("population2.csv", function(error, data) {

                        // Convert strings to numbers. Using + operator.
                        data.forEach(function(d) {
                            d.worktime = +d.worktime;
                            d.w_count = +d.w_count;
                            d.week = +d.week;
                        });

                        // Compute the extent of the data set in week and w_counts.
                        var week1 = d3.max(data, function(d) { return d.week; }),//max week: 90
                                w_count0 = d3.min(data, function(d) { return d.w_count; }),//min w_count: 1850
                                w_count1 = d3.max(data, function(d) { return d.w_count; }),//max w_count: 2000
                                w_count = w_count1;

                        // Update the scale domains.
                        x.domain([w_count1-week1,w_count1]);//1910,2000
                        y.domain([0, 24]);//0,11M??

                        // Produce a map from w_count and birthw_count to [male, female].
                        data = d3.nest()
                                .key(function(d) { return d.w_count; })
                                .key(function(d) { return d.w_count - d.week; })
                                .rollup(function(v) { return v.map(function(d) { return d.worktime; }); })
                                .map(data);

                        // Add an axis to show the population values.
                        svg.append("g")
                                .attr("class", "y axis")
                                .attr("transform", "translate(" + width + ",0)")
                                .call(yAxis)
                                .selectAll("g")
                                .filter(function(value) { return !value; })
                                .classed("zero", true);

                        // Add labeled rects for each birthw_count (so that no enter or exit is required).
                        var birthw_count = birthw_counts.selectAll(".birthw_count")
                                .data(d3.range(w_count0 - week1, w_count1 + 1))
                                .enter().append("g")
                                .attr("class", "birthw_count")
                                .attr("transform", function(birthw_count) { return "translate(" + x(birthw_count) + ",0)"; });

                        birthw_count.selectAll("rect")
                                .data(function(birthw_count) { return data[w_count][birthw_count] || [0, 0]; })
                                .enter().append("rect")
                                .attr("x", -barWidth / 2)
                                .attr("width", barWidth)
                                .attr("y", y)
                                .attr("height", function(value) { return height - y(value); });

                        // Add labels to show birthw_count.
                        birthw_count.append("text")
                                .attr("y", height - 4)
                                .text(function(birthw_count) { return birthw_count; });

                        // Add labels to show week (separate; not animated).
                        svg.selectAll(".week")
                                .data(d3.range(0, week1 + 1, 1))
                                .enter().append("text")
                                .attr("class", "week")
                                .attr("x", function(week) { return x(w_count - week); })
                                .attr("y", height + 4)
                                .attr("dy", ".71em")
                                .text(function(week) { return week; });

                        // Allow the arrow keys to change the displayed w_count.
                        window.focus();
                        d3.select(window).on("keydown", function() {
                            var w_count_after;
                            switch (d3.event.keyCode) {
                                case 37:
                                    switch(w_count%100) {
                                        case 31: w_count_after = w_count-130; break;
                                        case 30: w_count_after = w_count-129; break;
                                        default: w_count_after = w_count - 7;
                                    }
                                    w_count = Math.max(w_count0, w_count_after); weeks--; break;
                                case 39:
                                    switch(w_count%100) {
                                        case 31: w_count_after = w_count+70; break;
                                        case 30: w_count_after = w_count+71; break;
                                        default: w_count_after = w_count + 7;
                                    }
                                    w_count = Math.min(w_count1, w_count_after); weeks++; break;
                            }
                            update();
                        });

                        function update() {
                            if (!(w_count in data)) return;
                            title.text(weeks+ "weeks");

                            birthw_counts.transition()
                                    .duration(750)
                                    .attr("transform", "translate(" + (x(w_count1) - x(w_count)) + ",0)");

                            birthw_count.selectAll("rect")
                                    .data(function(birthw_count) { return data[w_count][birthw_count] || [0, 0]; })
                                    .transition()
                                    .duration(750)
                                    .attr("y", y)
                                    .attr("height", function(value) { return height - y(value); });
                        }
                    });


                </script>

            </div>

