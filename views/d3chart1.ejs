                <meta charset="utf-8">
                <style>
                    svg {
                        font: 10px sans-serif;
                    }

                    .y.axis path {
                        display: none;
                    }

                    .y.axis line {
                        stroke: #fff;
                        stroke-opacity: .2;
                        shape-rendering: crispEdges;
                    }

